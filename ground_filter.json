{
    "pipeline": [{"type": "readers.las",
                  "filename": "infile.las"},
                 {"type": "filters.outlier",
                  "method": "statistical",
                  "mean_k": 8,
                  "multiplier": 3.0},
                 {"type": "filters.smrf",
                  "cell": 0.1,
                  "scalar": 1.25,
                  "slope": 0.05,
                  "threshold": 0.1,
                  "window": 10.0,
                  "ignore": "Classification[7:7]"},
                 {"type": "filters.csf", 
                  "resolution": 1.0, 
                  "step": 6.5, 
                  "smooth": true,
                  "threshold": 0.5, 
                  "rigidness": 3, 
                  "iterations": 500},
                 {"type": "filters.range",
                  "limits": "Classification[2:2]"},
                 {"type": "writers.las",
                  "filename":  "outfile.las"}
                 ]
}